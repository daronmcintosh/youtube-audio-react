  "dependencies": {
    "@isolution/youtube-audio-stream": "^1.3.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "googleapis": "^43.0.0",
    "helmet": "^3.21.1",
    "heroku-ssl-redirect": "0.0.4",
    "loglevel": "^1.6.4",
    "memory-cache": "^0.2.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pm2": "^3.5.1",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
